		<cmdsynopsis>
			<command>base64</command> 
			
			<arg></arg>
			<arg></arg>
			<arg></arg>
		</cmdsynopsis>
	</refsynopsisdiv>
	
	
	
	<!--
		Full documentation.
		
		Equivalent to a manpage.
		
		@see
			http://www.docbook.org/tdg5/en/html/refsection.html
