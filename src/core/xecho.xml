<?xml version="1.0" encoding="UTF-8"?>
<refentry
	xmlns	="http://docbook.org/ns/docbook"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink"
	
	version	="5.0"
	label	="xecho" 
	role	="core"
>
	<refmeta>
		<refentrytitle>
			<command>xecho</command>
		</refentrytitle>
		<manvolnum>1</manvolnum>
	</refmeta>

	<refnamediv>
		<refname>
		    <command>xecho</command>
		</refname>
		<refpurpose>Print arguments as XML</refpurpose>
	</refnamediv>


	<refsynopsisdiv>
		<cmdsynopsis>
			<command>xecho</command>
			
		    <arg>-n</arg>
		    
		    <arg>
		        <group>
		            <arg choice="plain">-p</arg>
		            <arg choice="plain">-port</arg>
		        </group>
		        <replaceable>PORT</replaceable>
		    </arg>
		    
		    <arg choice="req" rep="repeat"><replaceable>ARG</replaceable></arg>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the command executed successfully, <returnvalue>1</returnvalue> if there was an error.</para>
    </refsection>

	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandXEcho</uri>
		</para>
	</refsection>
</refentry>
