<?xml version="1.0" encoding="UTF-8"?>
<refentry
	xmlns	="http://docbook.org/ns/docbook"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink"
	
	version	="5.0"
	role	="core streaming"
	label	="xcat" 
>
	<refmeta>
		<refentrytitle>
			<command>xcat</command>
		</refentrytitle>
		<manvolnum>1</manvolnum>
	</refmeta>

	<refnamediv>
		<refname>
		    <command>xcat</command>
		</refname>
		<refpurpose>Concatenate XML documents</refpurpose>
	</refnamediv>


	<refsynopsisdiv>
		<cmdsynopsis>
			<command>xcat</command>
			
		    <arg>
			    -w
			    <group choice="req">
			        <arg choice="plain"><replaceable>STRING</replaceable></arg>
			        <arg choice="plain"><replaceable>ELEMENT</replaceable></arg>
			    </group>
			</arg>
		    
		    <sbr/>
		    
		    <arg>-r</arg>
		    
		    <arg rep="repeat"><replaceable>FILE</replaceable></arg>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the command executed successfully, <returnvalue>1</returnvalue> if there was an error.</para>
    </refsection>

	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandXcat</uri>
		</para>
	</refsection>
</refentry>
