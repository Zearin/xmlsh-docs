<?xml version="1.0" encoding="UTF-8"?>
<refentry
	xmlns	="http://docbook.org/ns/docbook"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink"
	
	version	="5.0"
	label	="xgetopts" 
	role	="core"
>
	<refmeta>
		<refentrytitle>
			<command>xgetopts</command>
		</refentrytitle>
		<manvolnum>1</manvolnum>
	</refmeta>

	<refnamediv>
		<refname>
		    <command>xgetopts</command>
		</refname>
		<refpurpose>Parse arguments into XML</refpurpose>
	</refnamediv>


	<refsynopsisdiv>
		<cmdsynopsis>
			<command>xgetopts</command>
			
		    <group>
			    <arg choice="plain">-a</arg>
			    <arg choice="plain">-argindex</arg>
			</group>
		    
		    <group>
			    <arg choice="plain">-o</arg>
			    <arg choice="plain">-optdef</arg>
		        <replaceable>OPTSTRING</replaceable>
			</group>
		    
		    <group>
			    <arg choice="plain">-c</arg>
			    <arg choice="plain">-command</arg>
		        <replaceable>COMMAND_NAME</replaceable>
			</group>
		    
		    <group>
			    <arg choice="plain">-p</arg>
			    <arg choice="plain">-passthrough</arg>
		        <replaceable>OPTSTRING</replaceable>
			</group>
		    
		    <group>
			    <arg choice="plain">-i</arg>
			    <arg choice="plain">-ignore</arg>
		        <replaceable>OPTSTRING</replaceable>
			</group>
		    
		    
		    
	        <group>
	            <group>
                    <arg choice="plain">-s</arg>
                    <arg choice="plain">-serialize</arg>
                </group>
	            
                <group>
                    <arg choice="plain">+s</arg>
                    <arg choice="plain">+serialize</arg>
                </group>
	        </group>
		    
		    <group>
	            <group>
                    <arg choice="plain">-ps</arg>
                    <arg choice="plain">-pass-serialize</arg>
                </group>
	            
                <group>
                    <arg choice="plain">+ps</arg>
                    <arg choice="plain">+pass-serialize</arg>
                </group>
	        </group>
		    
		    <arg>-noargs</arg>
		    <arg>-novalues</arg>
		    
		    
		    <sbr/>
		    
		    <arg>--</arg>
		    
		    <sbr/>
		    <arg choice="req" rep="repeat"><replaceable>ARG</replaceable></arg>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the command executed successfully, <returnvalue>1</returnvalue> if there was an error.</para>
    </refsection>

	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandXgetopts</uri>
		</para>
	</refsection>
</refentry>
