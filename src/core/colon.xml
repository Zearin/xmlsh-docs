<?xml version="1.0" encoding="UTF-8"?>
<refentry
	xmlns	="http://docbook.org/ns/docbook"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink"
	
	version	="5.0"
	remap   ="command"
	label	=":"
	role	="core"
>
	
	<refmeta>
		<refentrytitle>
			<command>:</command>
		</refentrytitle>
		<manvolnum>1</manvolnum>
	</refmeta>

	<refnamediv>
		<refname>
		    <command>:</command>
		</refname>
	    <refname>Colon</refname>
		<refpurpose>
			The null command. It can be used in places where a command is required syntatcially but none is needed. 
		</refpurpose>
	</refnamediv>

	<refsynopsisdiv>
		<cmdsynopsis>
			<command>:</command>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the command executed successfully, <returnvalue>1</returnvalue> if there was an error.</para>
    </refsection>
    
	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandColon</uri>
		</para>
	</refsection>
</refentry>
