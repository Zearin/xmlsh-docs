<?xml version="1.0" encoding="UTF-8"?>
<refentry 
    xmlns="http://docbook.org/ns/docbook" 
    xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink" 
	
	version="5.0" 
	label="xmove" 
	role="core"
>
	
    <refmeta>
        <refentrytitle>
            <command>xmove</command>
        </refentrytitle>
        <manvolnum>1</manvolnum>
    </refmeta>

	<refnamediv>
		<refname>
		    <command>xmove</command>
		</refname>
		<refpurpose>Move (and/or rename) XML files, based on an expression in the file</refpurpose>
	</refnamediv>


	<refsynopsisdiv>
		<cmdsynopsis>
			<command>xmove</command>
			
		    <arg>
			    <group choice="req">
			        <arg choice="plain">-x</arg>
			        <arg choice="plain">-xpath</arg>
			    </group>
			    <replaceable>XPATH_EXPR</replaceable>
			</arg>
		    
		    <arg>
		        <group choice="req">
		            <arg choice="plain">-d</arg>
		            <arg choice="plain">-dir</arg>
		        </group>
		        <replaceable>DIR</replaceable>
		    </arg>
		    
		    <arg>-ns <replaceable>NAMESPACE</replaceable></arg>
		    
		    <arg>-f</arg>
		    
		    <arg>-mkdir</arg>
		    
		    <arg choice="req" rep="repeat"><replaceable>FILES</replaceable></arg>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the command executed successfully, <returnvalue>1</returnvalue> if there was an error.</para>
    </refsection>

	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandXmove</uri>
		</para>
	</refsection>
</refentry>