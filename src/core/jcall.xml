<?xml version="1.0" encoding="UTF-8"?>
<refentry
	xmlns	="http://docbook.org/ns/docbook"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink"
	
	version	="5.0"
	label	="jcall" 
	role	="core"
>
	<refmeta>
		<refentrytitle>
			<command>jcall</command>
		</refentrytitle>
		<manvolnum>1</manvolnum>
	</refmeta>

	<refnamediv>
		<refname>
		    <command>jcall</command>
		</refname>
		<refpurpose>Call Java from within the same JVM</refpurpose>
	</refnamediv>


	<refsynopsisdiv>
		<cmdsynopsis>
			<command>jcall</command>
			
		    <arg choice="opt">			    
                <group>
                    <arg choice="plain">-cp</arg>
                    <arg choice="plain">-classpath</arg>
                </group>
		        
			    <arg choice="req"><replaceable>CLASSPATH</replaceable></arg>
			</arg>
		    
		    <sbr/>
		    
			<arg choice="req"><replaceable>CLASSNAME</replaceable></arg>
			<arg choice="opt" rep="repeat">
			    <replaceable>ARGS</replaceable>
			</arg>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the command executed successfully, <returnvalue>1</returnvalue> if there was an error.</para>
    </refsection>
    
	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandJcall</uri>
		</para>
	</refsection>
</refentry>
