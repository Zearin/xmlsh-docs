<?xml version="1.0" encoding="UTF-8"?>
<refentry
	xmlns	="http://docbook.org/ns/docbook"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink"
	
	version	="5.0"
	label	="log" 
	role	="core"
>
	<refmeta>
		<refentrytitle>
			<command>log</command>
		</refentrytitle>
		<manvolnum>1</manvolnum>
	</refmeta>

	<refnamediv>
		<refname>
		    <command>log</command>
		</refname>
		<refpurpose>Writes to the log4j log</refpurpose>
	</refnamediv>


	<refsynopsisdiv>
		<cmdsynopsis>
			<command>log</command>
		    
			<arg>
			    <group choice="req">
			        <arg choice="plain">-c</arg>
			        <arg choice="plain">-classname</arg>
			    </group>
			    <arg choice="req"><replaceable>CLASSNAME</replaceable></arg>
			</arg>
		    
		    <sbr/>
		    
		    <arg>
			    <group choice="req">
			        <arg choice="plain">-p</arg>
			        <arg choice="plain">-priority</arg>
			    </group>
			    <arg choice="req"><replaceable>PRIORITY</replaceable></arg>
			</arg>
		    
		    <sbr/>
		    
		    <arg rep="repeat"><replaceable>ARGS</replaceable></arg>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the command executed successfully, <returnvalue>1</returnvalue> if there was an error.</para>
    </refsection>

	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandLog</uri>
		</para>
	</refsection>
</refentry>
