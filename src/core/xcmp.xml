<?xml version="1.0" encoding="UTF-8"?>
<refentry
	xmlns	="http://docbook.org/ns/docbook"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink"
	
	version	="5.0"
	role	="core streaming"
	label	="xcmp" 
>
	<refmeta>
		<refentrytitle>
			<command>xcmp</command>
		</refentrytitle>
		<manvolnum>1</manvolnum>
	</refmeta>

	<refnamediv>
		<refname>
		    <command>xcmp</command>
		</refname>
		<refpurpose>Compare text, binary, or XML data</refpurpose>
	</refnamediv>


	<refsynopsisdiv>
		<cmdsynopsis>
			<command>xcmp</command>
		    
		    <arg>-x</arg>
		    <arg>-b</arg>
		    <arg>-c</arg>
		    <arg>-p</arg>
		    <arg>-n</arg>
		    
			<arg choice="req"><replaceable>FILE1</replaceable></arg>
		    <arg choice="req"><replaceable>FILE2</replaceable></arg>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the files are considered identical, <returnvalue>1</returnvalue> otherwise.</para>
    </refsection>

	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandXcmp</uri>
		</para>
	</refsection>
</refentry>
