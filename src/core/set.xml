<?xml version="1.0" encoding="UTF-8"?>
<refentry
	xmlns	="http://docbook.org/ns/docbook"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink"
	
	version	="5.0"
	label	="set" 
	role	="core"
>
	<refmeta>
		<refentrytitle>
			<command>set</command>
		</refentrytitle>
		<manvolnum>1</manvolnum>
	</refmeta>

	<refnamediv>
		<refname>
		    <command>set</command>
		</refname>
		<refpurpose>Set positional parameters and view variables</refpurpose>
	</refnamediv>


	<refsynopsisdiv>
		<cmdsynopsis>
			<command>set</command>
			
		    <arg choice="opt">
			    <synopfragmentref linkend="options">OPTIONS</synopfragmentref>
			</arg>
		    
		    <arg choice="opt" rep="repeat">
		        <replaceable>param</replaceable>
		    </arg>
		    
		    <synopfragment xml:id="options">
		        <arg>-x</arg>
		        <arg>-v</arg>
		        <arg>-e</arg>
		        <arg>-xpipe</arg>
		    </synopfragment>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the command executed successfully, <returnvalue>1</returnvalue> if there was an error.</para>
    </refsection>

	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandSet</uri>
		</para>
	</refsection>
</refentry>
