<?xml version="1.0" encoding="UTF-8"?>
<refentry
	xmlns	="http://docbook.org/ns/docbook"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xl="http://www.w3.org/1999/xlink"
	
	version	="5.0"
	label	="wait" 
	role	="core"
>
	<refmeta>
		<refentrytitle>
			<command>wait</command>
		</refentrytitle>
		<manvolnum>1</manvolnum>
	</refmeta>

	<refnamediv>
		<refname>
		    <command>wait</command>
		</refname>
		<refpurpose>Wait for jobs to complete</refpurpose>
	</refnamediv>


	<refsynopsisdiv>
		<cmdsynopsis>
			<command>wait</command>
			<arg>
			    <group choice="plain">
			        <arg choice="plain">-m</arg>
			        <arg choice="plain">-atmost</arg>
			    </group>
			    <replaceable>N</replaceable>
			</arg>
		    <arg choice="opt" rep="repeat"><replaceable>JOB_ID</replaceable></arg>
		</cmdsynopsis>
	</refsynopsisdiv>
    
    <refsection>
        <title>Return Value</title>
        <para>Returns <returnvalue>0</returnvalue> if the command executed successfully, <returnvalue>1</returnvalue> if there was an error.</para>
    </refsection>

	<refsection>
		<title>See Also</title>
		<para>
			<uri>http://www.xmlsh.org/CommandWait</uri>
		</para>
	</refsection>
</refentry>
